   <xs:import namespace="http://xml.ra.se/xlink" schemaLocation="http://xml.ra.se/EAD/RA_EAD_xlink.xsd"/>
   <xs:attributeGroup
      name="am.date.normal">
      <xs:attribute name="normal">
         <xs:simpleType>
            <xs:restriction base="xs:token">
               <xs:pattern value="(\-?(0|1|2)([0-9]{3})(((01|02|03|04|05|06|07|08|09|10|11|12)((0[1-9])|((1|2)[0-9])|(3[0-1])))|\-((01|02|03|04|05|06|07|08|09|10|11|12)(\-((0[1-9])|((1|2)[0-9])|(3[0-1])))?))?)(/\-?(0|1|2)([0-9]{3})(((01|02|03|04|05|06|07|08|09|10|11|12)((0[1-9])|((1|2)[0-9])|(3[0-1])))|\-((01|02|03|04|05|06|07|08|09|10|11|12)(\-((0[1-9])|((1|2)[0-9])|(3[0-1])))?))?)?"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
   </xs:attributeGroup>
   <xs:simpleType name="data.repositorycode">
      <xs:restriction base="xs:token">
        <!--\p{IsBasicLatin} -> a,b,c
        \P{Lu} -> a,b,c,1,2,3
        \P{L} -> any letter 
        ([\p{IsBasicLatin}-[\P{Lu}]]{2}) -> A,B,C,a,b,c (två tecken)
        alternativt
        ([\p{IsBasicLatin}-[\P{L}]]{1,4}) -> A,B,C,a,b,c (ett till fyra tecken)
        obligatoriskt bindestreck
        [\-:/\p{IsBasicLatin}-[^\p{L}0-9]]{1,11} -> A,B,C,a,b,c,1,2,3 (ett till elva tecken)
        -->
         <xs:pattern value="(([\p{IsBasicLatin}-[\P{Lu}]]{2})|([\p{IsBasicLatin}-[\P{L}]]{1,4}))(-[\-:/\p{IsBasicLatin}-[^\p{L}0-9]]{1,11})"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:attributeGroup name="am.mainagencycode">
     <xs:annotation>
    <xs:documentation xml:lang="sv">
      am.mainagencycode: Justerat så att det endast ska vara type="xs:NMTOKEN" istället för type="data.repositorycode" för mainagencycode. Karin Bredenberg, 2006-07-26
    </xs:documentation>
  </xs:annotation>
      <xs:attribute name="mainagencycode" type="xs:NMTOKEN"/>
   </xs:attributeGroup>
   <xs:attributeGroup name="am.repositorycode">
     <xs:annotation>
    <xs:documentation xml:lang="sv">
      am.repositorycode: Justerat så att det endast ska vara type="xs:NMTOKEN" istället för type="data.repositorycode" för respositorycode. Karin Bredenberg, 2006-07-26
    </xs:documentation>
  </xs:annotation>
      <xs:attribute name="repositorycode" type="xs:NMTOKEN"/>
   </xs:attributeGroup>
